<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" fx:id="apLoginWindow" prefHeight="450.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="awesomechatapp.FXMLLoginWindowController">
    <children>
        <Label fx:id="myLabel" layoutX="174.0" layoutY="390.0" minHeight="16" minWidth="69" />
      <Hyperlink fx:id="linkSignUp" alignment="CENTER" focusTraversable="false" layoutX="175.0" layoutY="350.0" onAction="#openSignUpWindow" text="or Sign Up" textOverrun="CLIP" underline="true" />
      <Label layoutX="136.0" layoutY="257.0" text="Password:" />
      <TextField fx:id="tfUsername" layoutX="133.0" layoutY="215.0" />
      <PasswordField fx:id="pfPassword" layoutX="133.0" layoutY="274.0" prefHeight="25.0" prefWidth="149.0" />
        <Button fx:id="btnSignIn" layoutX="173.0" layoutY="314.0" onAction="#clickedSignIn" prefHeight="30.0" prefWidth="70.0" text="Sign In" />
      <Label id="lblUsername" layoutX="135.0" layoutY="197.0" text="Username:" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets />
         </GridPane.margin>
      </Label>
      <Hyperlink fx:id="linkForgotPassword" focusTraversable="false" layoutX="143.0" layoutY="425.0" onAction="#clickedForgotPassword" text="Forgot your password?" />
      <ImageView fitHeight="140.0" fitWidth="140.0" layoutX="135.0" layoutY="41.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@logo.png" />
         </image>
      </ImageView>
    </children>
</AnchorPane>
